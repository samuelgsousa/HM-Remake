{
    "headers": [
      {
        "source": "/(.*)\\.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/dist/hm-remake" }
    ]
  }
  